{
  "name": "@danmarshall/deckgl-typings",
  "version": "4.9.22",
  "description": "TypeScript declaration files for deck.gl",
  "types": "deck.gl/index.d.ts",
  "scripts": {
    "build": "node ./scripts/convert.js",
    "test": "tsc -p ./test",
    "lint:format": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,yml,yaml,css,scss,md,mdx,html}\"",
    "postinstall": "indefinitely-typed --folder deck.gl --folder deck.gl__aggregation-layers --folder deck.gl__arcgis --folder deck.gl__carto --folder deck.gl__core --folder deck.gl__extensions --folder deck.gl__geo-layers --folder deck.gl__google-maps --folder deck.gl__json --folder deck.gl__layers --folder deck.gl__mapbox --folder deck.gl__mesh-layers --folder deck.gl__react --folder luma.gl__constants --folder luma.gl__core --folder luma.gl__gltools --folder luma.gl__webgl --folder luma.gl__webgl-state-tracker --folder math.gl --folder math.gl__core",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,json,yml,yaml,css,scss,md,mdx,html}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danmarshall/deckgl-typings.git"
  },
  "keywords": [
    "deck.gl",
    "TypeScript"
  ],
  "author": "Dan Marshall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danmarshall/deckgl-typings/issues"
  },
  "homepage": "https://github.com/danmarshall/deckgl-typings#readme",
  "devDependencies": {
    "@deck.gl/arcgis": "^8.4.9",
    "@luma.gl/core": "*",
    "@luma.gl/webgl": "*",
    "@luma.gl/webgl-state-tracker": "*",
    "@types/node": "*",
    "@types/react": "*",
    "deck.gl": "*",
    "dts-generator": "^3.0.0",
    "glob": "^7.1.5",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "math.gl": "*",
    "prettier": "^2.5.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@types/hammerjs": "^2.0.36",
    "@types/react": "*",
    "indefinitely-typed": "^1.1.0"
  }
}
